{
  "dependencies": {
    "pm2": "~6.0.14"
  },
  "devDependencies": {
    "@eslint/compat": "~2.0.0",
    "@eslint/js": "~9.39.1",
    "eslint": "~9.39.1",
    "eslint-config-prettier": "~10.1.8",
    "eslint-plugin-promise": "~7.2.1",
    "eslint-plugin-regexp": "~2.10.0",
    "eslint-plugin-simple-import-sort": "~12.1.1",
    "eslint-plugin-unicorn": "~62.0.0",
    "globals": "~16.5.0",
    "husky": "~9.1.7",
    "kysely": "0.28.9",
    "kysely-codegen": "0.19.0",
    "lint-staged": "~16.2.7",
    "prettier": "~3.7.4",
    "tsx": "~4.21.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "~8.48.1",
    "vitest": "~4.0.16"
  },
  "name": "ymh8",
  "private": true,
  "scripts": {
    "build": "pnpm -r run build",
    "build:packages": "pnpm -w -r --filter \"./packages/\" --filter \"!./apps/\" run build",
    "clean": "rm -rf apps/*/dist packages/*/dist",
    "codegen": "kysely-codegen",
    "dev": "pnpm run -r dev",
    "dev:apps": "pnpm -w -r --filter \"./apps/\" run dev --parallel",
    "lint": "eslint .",
    "prepare": "husky",
    "start": "pm2 start ecosystem.config.cjs",
    "stop": "pm2 stop ecosystem.config.cjs",
    "test": "vitest"
  },
  "type": "module"
}
